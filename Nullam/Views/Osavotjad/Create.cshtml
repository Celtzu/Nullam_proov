@model Nullam.Models.OsavotjadModel

<h4>Osavõtja info</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Eesnimi" class="control-label"></label>
                <input asp-for="Eesnimi" class="form-control" />
                <span asp-validation-for="Eesnimi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Perenimi" class="control-label"></label>
                <input asp-for="Perenimi" class="form-control" />
                <span asp-validation-for="Perenimi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Isikukood" class="control-label"></label>
                <input asp-for="Isikukood" class="form-control" />
                <span asp-validation-for="Isikukood" class="text-danger"></span>
            </div>
            <select name="number">
                <option value=""></option>
                <option value="1">Sularaha</option>
                <option value="2">Pangaülekanne</option>
            </select>
            <div class="form-group">
                <label asp-for="Lisainfo" class="control-label"></label>
                <input asp-for="Lisainfo" class="form-control" />
                <span asp-validation-for="Lisainfo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
